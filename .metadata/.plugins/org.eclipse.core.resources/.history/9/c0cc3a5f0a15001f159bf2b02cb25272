public class Impetus {

	public static void main(String[] args) {

		String s = "123";
		boolean flag = false;
		for (int i = 0; i < s.length(); i++) {

			char c = s.charAt(i);

			if (c >= 48 && c <= 57) {
				flag = true;
				break;

			}

		}
		if (flag) {
			System.out.println("Not a valid number");
		} else {
			System.out.println("valid number");
		}

	}

}
